		"pocketbase": "^0.21.2",
		"radix-icons-svelte": "^1.2.1",
		"svelte-headless-table": "^0.18.2",
		"svelte-sonner": "^0.3.22",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1"
	},
	"lint-staged": {
		"*.{js,ts,svelte,html,css,scss,postcss,md,json}": [
